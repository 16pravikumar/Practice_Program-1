package javap;
/*Find the highest number from the array
*/
public class javap  {
	
	public static int getHigh(int arr[]) {
		int high=arr[0];
		for(int i=0;i<arr.length;i++) {
			if(high<arr[i])
				high=arr[i];
		}
		return high;
	}
	public static void main(String[] args) {
		
		int arr[]= {10,6,8,2,21};
		System.out.println(getHigh(arr));
	}
}
/* 21
 */
